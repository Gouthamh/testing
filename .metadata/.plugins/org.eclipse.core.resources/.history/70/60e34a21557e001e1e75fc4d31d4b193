	static void isprime() {
		int number=16;
		if(number==0 || number==1) {
			System.out.println(false);
		}
		if(number==2) {
			System.out.println(true);
		}
		else {
			for(int i=2;i<number/2;i++) {
				if(number%i==0) {
					System.out.println(i + " " +false);
				}
			}
			System.out.println(true);
		}
	}

	public static void main(String[] args) {
//		reserve();
